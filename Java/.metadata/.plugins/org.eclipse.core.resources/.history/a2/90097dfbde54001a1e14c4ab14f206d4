package br.com.loiane.exercicio4;

public class Exercicio4 {

	public static void main(String[] args) {
		int paisA = 10;
		int paisB = 100;
		
		double taxaPaisA = paisA * 0.1;
		double taxaPaisB = paisB * 0.01;
		
		int anos = 0;
		while (paisA <= paisB) {
			
			paisA += taxaPaisA;
			paisB += taxaPaisB;
			anos++;
		}
		System.out.println("ano : " + anos);
		
		
	}

}
